{
  "name": "call-your-congresspeople",
  "version": "1.0.0",
  "description": "A phone number that automatically dials your representatives",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon --watch",
    "test": "mocha --recursive test/",
    "test:watch": "chokidar '**/*.js' --ignore node_modules --initial -c 'mocha --recursive test/'",
    "tunnel": "ssh -vvv $TUNNEL_HOST -nNT -o ServerAliveInterval=15 -R $TUNNEL_PORT:localhost:17738",
    "lint": "eslint .",
    "lint-fix": "eslint --fix ."
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-validator": "^2.20.4",
    "method-override": "^2.3.5",
    "morgan": "^1.7.0",
    "request": "^2.72.0",
    "twilio": "^2.9.1"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "debug": "^2.6.0",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "supertest": "^2.0.1"
  },
  "engines": {
    "node": "6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/StayWokeOrg/general-congress-hotline"
  },
  "license": "MIT"
}
